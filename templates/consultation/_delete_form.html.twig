<form method="post" action="{{ path('app_consultation_delete', {'id': consultation.id}) }}" onsubmit="return confirm('Voulez vouz supprimer cette Consultation?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ consultation.id) }}">
    <button class="btn btn-danger rounded-pill">Supprimer</button>
</form>
